{
  "name": "mini-crm-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/mini-crm-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/mini-crm-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "mini-crm:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "mini-crm:serve:production"
        },
        "ci": {
          "devServerTarget": "mini-crm:serve-static"
        }
      }
    },
    "open-cypress": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/mini-crm-e2e/cypress.config.ts",
        "testingType": "e2e",
        "watch": true,
        "headed": true,
        "devServerTarget": "mini-crm:serve:development"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  },
  "tags": ["type:e2e"],
  "implicitDependencies": ["mini-crm"]
}

